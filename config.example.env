# CB Product Ratings Widget - Environment Configuration

# API Configuration
REACT_APP_API_BASE_URL=https://your-api.com

# Feature Flags
REACT_APP_FEATURE_ANALYTICS=true
REACT_APP_FEATURE_CACHING=true
REACT_APP_FEATURE_OFFLINE=false
REACT_APP_FEATURE_DARK_MODE=false

# Performance Configuration
REACT_APP_CACHE_TTL=300000
REACT_APP_MAX_CACHE_SIZE=100
REACT_APP_DEBOUNCE_DELAY=300

# Analytics Configuration
REACT_APP_ANALYTICS_ENABLED=true
REACT_APP_ANALYTICS_TRACKING_ID=your-tracking-id

# Development Configuration
REACT_APP_USE_MOCK_DATA=false
REACT_APP_DEBUG_MODE=false

# Security Configuration
REACT_APP_ENABLE_CSP=true
REACT_APP_ALLOWED_ORIGINS=https://your-domain.com

# Performance Monitoring
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_PERFORMANCE_THRESHOLD=1000
